<Bindings>
	<!-- BlizzMove -->
	<Binding name="KPACKMOVEFRAME" header="KPACKBLIZZMOVE">
		if KPack.BlizzMove then KPack.BlizzMove:Toggle() end
	</Binding>
	<Binding name="CLICK QuickButtonFrame:LeftButton" header="KPACKQUICKBUTTON" />

	<!-- Random Mount -->
	<Binding name="KPACKRANDOMMOUNT_REG" header="KPACKRANDOMMOUNT">
		if KPack.RandomMount then KPack.RandomMount:FindMount("") end
	</Binding>
	<Binding name="KPACKRANDOMMOUNT_RIDING">
		if KPack.RandomMount then KPack.RandomMount:FindMount("riding") end
	</Binding>
	<Binding name="KPACKRANDOMMOUNT_FLYING">
		if KPack.RandomMount then KPack.RandomMount:FindMount("flying") end
	</Binding>

	<!-- MouseOver Raid Icons -->
	<Binding name="KPACKAUTOMATE_1" header="KPACKAUTOMATE">SetRaidTarget("MOUSEOVER", 1)</Binding>
	<Binding name="KPACKAUTOMATE_2">SetRaidTarget("MOUSEOVER", 2)</Binding>
	<Binding name="KPACKAUTOMATE_3">SetRaidTarget("MOUSEOVER", 3)</Binding>
	<Binding name="KPACKAUTOMATE_4">SetRaidTarget("MOUSEOVER", 4)</Binding>
	<Binding name="KPACKAUTOMATE_5">SetRaidTarget("MOUSEOVER", 5)</Binding>
	<Binding name="KPACKAUTOMATE_6">SetRaidTarget("MOUSEOVER", 6)</Binding>
	<Binding name="KPACKAUTOMATE_7">SetRaidTarget("MOUSEOVER", 7)</Binding>
	<Binding name="KPACKAUTOMATE_8">SetRaidTarget("MOUSEOVER", 8)</Binding>
	<Binding name="KPACKAUTOMATE_0">SetRaidTarget("MOUSEOVER", 0)</Binding>

	<!-- MoveAnything -->
	<Binding name="Toggle MA window" header="KPACKMOVEANYTHING">
		if KPack.MA then KPack.MA:ToggleOptionsMenu() end
	</Binding>
	<Binding name="Move frame - Safe">
		if KPack.MA then KPack.MA:SafeMoveFrameAtCursor() end
	</Binding>
	<Binding name="Hide frame - Safe">
		if KPack.MA then KPack.MA:SafeHideFrameAtCursor() end
	</Binding>
	<Binding name="Reset frame - Safe">
		if KPack.MA then KPack.MA:SafeResetFrameAtCursor() end
	</Binding>
	<Binding name="Move frame - Exact">
		if KPack.MA then KPack.MA:MoveFrameAtCursor() end
	</Binding>
	<Binding name="Hide frame - Exact">
		if KPack.MA then KPack.MA:HideFrameAtCursor() end
	</Binding>
	<Binding name="Reset frame - Exact">
		if KPack.MA then KPack.MA:ResetFrameAtCursor() end
	</Binding>
	<Binding name="Update positions">
		if KPack.MA then KPack.MA:SyncAllFrames() end
	</Binding>
	<Binding name="Show frame info">
		if KPack.MA then KPack.MA:DebugFrameAtCursor() end
	</Binding>

	<!-- Combuctor -->
	<Binding name="COMBUCTOR_TOGGLE_INVENTORY" description="BINDING_NAME_COMBUCTOR_TOGGLE_INVENTORY" header="COMBUCTOR">
		if KPack.Combuctor then Combuctor:Toggle(0) end
	</Binding>
	<Binding name="COMBUCTOR_TOGGLE_BANK" description="BINDING_NAME_COMBUCTOR_TOGGLE_BANK">
		if KPack.Combuctor then Combuctor:Toggle(-1) end
	</Binding>

</Bindings>