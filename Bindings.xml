<Bindings>
	<!-- BlizzMove -->
	<Binding name="KPACKMOVEFRAME" header="KPACKBLIZZMOVE">KPack.BlizzMove:Toggle()</Binding>
	<Binding name="CLICK QuickButtonFrame:LeftButton" header="KPACKQUICKBUTTON" />

	<!-- Random Mount -->
	<Binding name="KPACKRANDOMMOUNT_REG" header="KPACKRANDOMMOUNT">KPack.RandomMount:FindMount("")</Binding>
	<Binding name="KPACKRANDOMMOUNT_RIDING">KPack.RandomMount:FindMount("riding")</Binding>
	<Binding name="KPACKRANDOMMOUNT_FLYING">KPack.RandomMount:FindMount("flying")</Binding>

	<!-- MouseOver Raid Icons -->
	<Binding name="KPACKAUTOMATE_1" header="KPACKAUTOMATE">SetRaidTarget("MOUSEOVER", 1)</Binding>
	<Binding name="KPACKAUTOMATE_2">SetRaidTarget("MOUSEOVER", 2)</Binding>
	<Binding name="KPACKAUTOMATE_3">SetRaidTarget("MOUSEOVER", 3)</Binding>
	<Binding name="KPACKAUTOMATE_4">SetRaidTarget("MOUSEOVER", 4)</Binding>
	<Binding name="KPACKAUTOMATE_5">SetRaidTarget("MOUSEOVER", 5)</Binding>
	<Binding name="KPACKAUTOMATE_6">SetRaidTarget("MOUSEOVER", 6)</Binding>
	<Binding name="KPACKAUTOMATE_7">SetRaidTarget("MOUSEOVER", 7)</Binding>
	<Binding name="KPACKAUTOMATE_8">SetRaidTarget("MOUSEOVER", 8)</Binding>
	<Binding name="KPACKAUTOMATE_0">SetRaidTarget("MOUSEOVER", 0)</Binding>

	<!-- MoveAnything -->
	<Binding name="Toggle MA window" header="KPACKMOVEANYTHING">KPack.MA:ToggleOptionsMenu()</Binding>
	<Binding name="Move frame - Safe">KPack.MA:SafeMoveFrameAtCursor()</Binding>
	<Binding name="Hide frame - Safe">KPack.MA:SafeHideFrameAtCursor()</Binding>
	<Binding name="Reset frame - Safe">KPack.MA:SafeResetFrameAtCursor()</Binding>
	<Binding name="Move frame - Exact">KPack.MA:MoveFrameAtCursor()</Binding>
	<Binding name="Hide frame - Exact">KPack.MA:HideFrameAtCursor()</Binding>
	<Binding name="Reset frame - Exact">KPack.MA:ResetFrameAtCursor()</Binding>
	<Binding name="Update positions">KPack.MA:SyncAllFrames()</Binding>
	<Binding name="Show frame info">KPack.MA:DebugFrameAtCursor()</Binding>
</Bindings>